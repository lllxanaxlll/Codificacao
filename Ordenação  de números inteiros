// Ordenação de numeros inteiros 

#include <stdio.h>

int main() {
    int vetor[10];
    int i, j, aux;

    // Leitura dos 10 números
    printf("Digite 10 numeros inteiros:\n");
    for (i = 0; i < 10; i++) {
        scanf("%d", &vetor[i]);
    }

    printf("\nVetor antes da ordenacao:\n");
    for (i = 0; i < 10; i++) {
        printf("%d ", vetor[i]);
    }

    for (i = 0; i < 10 - 1; i++) {
        for (j = 0; j < 10 - 1 - i; j++) {
            if (vetor[j] > vetor[j + 1]) {
                aux = vetor[j];
                vetor[j] = vetor[j + 1];
                vetor[j + 1] = aux;
            }
        }
    }

    printf("\n\nVetor apos a ordenacao:\n");
    for (i = 0; i < 10; i++) {
        printf("%d ", vetor[i]);
    }

    printf("\n");

    return 0;
}
